<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="com.sts.module.STSBomPlugin.help"
         name="com.sts.module.STSBomPlugin.help"
         point="org.adempiere.base.IColumnCallout">
      <callout
            class="com.sts.callout.CalloutProductBOM"
            columnName="M_Product_ID"
            tableName="PP_Product_Bom_Sts">
      </callout>
   </extension>
   <extension
         id="com.sts.process.CoppyProduct"
         name="com.sts.process.CoppyProduct"
         point="org.adempiere.base.Process">
      <process
            class="com.sts.callout.CalloutProductBOM"
            priority="1">
      </process>
   </extension>
   <extension
         id="com.sts.modelvalidator.stsPPProductBomValidator"
         name="com.sts.modelvalidator.stsPPProductBomValidator"
         point="org.adempiere.base.ModelValidator">
      <listener
            class="com.sts.modelvalidator.stsPPProductBomValidator"
            priority="1">
      </listener>
   </extension>

</plugin>
